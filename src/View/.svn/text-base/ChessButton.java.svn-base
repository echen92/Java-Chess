package View;

import javax.swing.*;
import java.awt.*;

public class ChessButton extends JButton {
    Font font = new Font("DejaVu Sans", Font.PLAIN, 40);

    ChessButton(String s) {
        super(s);
        setFont(font);
        setHorizontalAlignment(SwingConstants.CENTER);
        setContentAreaFilled(true);
        setBorderPainted(true);
    }
}
